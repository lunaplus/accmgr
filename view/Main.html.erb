<div id="titles">
  <h2>収支管理メイン画面</h2>
  <div id ="welcome">
    Welcome <%= showname %> !
  </div>
</div>
<div id="menuarea">
  <ul id="normal" class="dropmenu">
    <li><a href="#">メニュー▼</a>
      <ul>
	<%= menuList %>
      </ul>
    </li>
  </ul>
</div>
<div id="inputarea">
  <form action="<%= actionUrl %>" method="post" accept-charset="UTF-8"
	autocomplete="off" name="inputarea">
    <div id="wpdate">
      <select name="yearsel"><%= yearsel %></select>
      /
      <select name="monthsel"><%= monthsel %></select>
      /
      <select name="datesel"><%= datesel %></select>
    </div>
    <div id="wpkind">
      <input type="radio" name="wpkinds" value="withdraw">収入<br>
      <input type="radio" name="wpkinds" value="payment">支出<br>
      <input type="radio" name="wpkinds" value="accounttransfer">口座移動<br>
    </div>
    <div id="expenditure">
      費目
      <select name="expenditures">
	<%= expsel %>
      </select>
    </div>
    <div id="account">
      支払区分
      <select name="withdrawFrom">
	<%= wFromsel %>
      </select><br>
      収入区分
      <select name="paymentTo">
	<%= pTosel %>
      </select>
    </div>
    <div id="amount">
      金額
      <input type="text" name="amounts" value="">円
    </div>
    <div id="paymentMonth">
      カード支払月
      <select name="payMonth">
	<option value="0" selected></option>
	<%= paymonthsel %>
      </select>
    </div>
    <div id="accremain">
      口座残額
      <span id="accremamount">-</span>円
    </div>
  </form>
</div>
