<div id="titles">
  <h2>統計画面</h2>
  <div id ="welcome">
    Welcome <%= showname %> !
  </div>
</div>
<%= menuList %>
<div id="errorarea">
  <%= errorstr %>
</div>
<div id="inputarea">
  <form name="inputarea" method="post" accept-charset="UTF-8"
	autocomplete="off">
    <div id="datetime">
      <select name="<%= SELYEAR %>" id="<%= SELYEAR %>">
	<%= selyear %>
      </select>年
      <select name="<%= SELMONTH %>" id="<%= SELMONTH %>">
	<%= selmonth %>
      </select>月の統計
    </div>
    <div id="target">
      <div id="owner">
	対象者
	<select name="<%= SELOWNER %>" id="<%= SELOWNER %>">
	  <%= selowner %>
	</select>
      </div>
      <div id="account">
	対象口座
	<select name="<%= SELACC %>" id="<%= SELACC %>">
	  <%= selacc %>
	</select>
      </div>
    </div>
    <div id="action">
      <input type="radio" name="action" id="display"
	     value="display" checked="checked">
      <label for="display">表示</label>
      <input type="radio" name="action" id="stats" value="stats">
      <label for="stats">集計</label>
    </div>
    <input type="submit" name="submit" value="実行">
  </form>
</div>
<div id="displayarea">
</div>
